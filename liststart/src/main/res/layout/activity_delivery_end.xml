                    <!-- 오른쪽 정보: 수량 -->
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="100개"
                        android:textSize="12sp"
                        android:gravity="end"
                        android:textColor="#083D80" />
                </LinearLayout>

                <!-- 리스트 항목4 -->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="3dp">

                    <!-- 체크박스 아이콘 -->
                    <ImageView
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:src="@drawable/ic_checked"
                        android:layout_marginEnd="3dp"
                        android:layout_marginTop="2dp"/>

                    <!-- 왼쪽 정보: 출고번호 -->
                    <TextView
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:text="출고번호 : 4"
                        android:textSize="14sp"

                        android:textColor="#000000" />

                    <!-- 왼쪽 정보: 회사명 -->
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="Tech Innovators"
                        android:textSize="12sp"
                        android:textColor="#083D80" />

                    <!-- 오른쪽 정보: 상품명 -->
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:text="검정색 의자"
                        android:textSize="12sp"
                        android:textColor="#000000" />

                    <!-- 오른쪽 정보: 수량 -->
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="100개"
                        android:textSize="12sp"
                        android:gravity="end"
                        android:textColor="#083D80" />
                </LinearLayout>

                <!-- 더보기 버튼 -->
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="더보기▼"
                    android:textSize="10sp"
                    android:gravity="center"
                    android:textColor="#000000" />

            </LinearLayout>


        </LinearLayout>



    <!-- 하단 네비게이션 바 -->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:orientation="vertical"
        android:background="#FFFFFF"
        android:layout_marginTop="10dp">

        <!-- 상단 구분선 -->
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#000000" /> <!-- 검정색 가로 구분선 -->

        <!-- 버튼 영역 -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:gravity="center"
            >

            <!-- 첫 번째 이미지 버튼 (홈) -->
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical"
                android:gravity="center">

                <!-- 선택 표시를 위한 파란색 선 -->
                <View
                    android:id="@+id/leftIndicator"
                    android:layout_width="50dp"
                    android:layout_height="5dp"
                    android:background="#B30058A3"
                    android:visibility="gone" /> <!-- 기본적으로 숨김 -->

                <!-- 아이콘 -->
                <ImageView
                    android:id="@+id/navImage1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:src="@drawable/ic_leftbutton"
                    android:contentDescription="홈"
                    android:background="?android:attr/selectableItemBackground"
                    android:padding="12dp"
                    android:scaleType="centerInside" />
            </LinearLayout>

            <!-- 버튼 사이 세로 구분선 -->
            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:background="#000000" /> <!-- 검정색 세로 구분선 -->

            <!-- 두 번째 이미지 버튼 (설정) -->
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical"
                android:gravity="center">

                <!-- 선택 표시를 위한 파란색 선 -->
                <View
                    android:id="@+id/rightIndicator"
                    android:layout_width="50dp"
                    android:layout_height="6dp"
                    android:background="#B30058A3"
                    android:visibility="visible" /> <!-- 기본적으로 숨김 -->

                <!-- 아이콘 -->
                <ImageView
                    android:id="@+id/navImage2"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:src="@drawable/ic_rightbutoon"
                    android:contentDescription="설정"
                    android:background="?android:attr/selectableItemBackground"
                    android:padding="12dp"
                    android:scaleType="centerInside" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>



</LinearLayout>
</LinearLayout>
